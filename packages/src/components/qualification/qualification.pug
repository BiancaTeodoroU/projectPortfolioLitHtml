mixin qualification()
    .tab-bar
        input#tab-1-rb.hidden(type='radio' name='tab-bar-item' checked='')
        .tab-container
            .trapezoid
            a.tab(href='#')
                label(for='tab-1-rb' class="u-color-pink") Educação
        input#tab-2-rb.hidden(type='radio' name='tab-bar-item')
        .tab-container
            .trapezoid
            a.tab(href='#')
                label(for='tab-2-rb' class="u-color-pink") Trabalho
        .texts
            .text1
                .text
                    .text1(class="u-text-center")
                        span(class="u-color-white u-db") Clotilde Veiga De Barros Escola Estadual 
                        span(class="u-color-white u-db") Ensino Fundamental e Médio 
                        date(class="u-color-white") 2014 - 2020
                    .c-qualification__border
                    .text2t(class="u-text-center")
                        span(class="u-color-white u-db") Faculdade de Análise e desenvolvimento de sistemas
                        span(class="u-color-white u-db") centro universitário toledo prudente
                        date(class="u-color-white") 2021 - 2023
            .text2
                .text2r   
                    .text2t(class="u-text-center")
                        span(class="u-color-white u-db") Jovem Aprendiz Auxiliar de Escritório em Geral
                        span(class="u-color-white u-db") Security Segurança - Trabalho
                        date(class="u-color-white") fev 2021 - abr 2022
                    .c-qualification__border
                    .text2t(class="u-text-center")
                        span(class="u-color-white u-db") Estágiaria em Engenharia de Software
                        span(class="u-color-white u-db") Foregon - Trabalho
                        date(class="u-color-white") abr de 2022 - dez de 2023
                    .c-qualification__border
                    .text2t(class="u-text-center")
                        span(class="u-color-white u-db") Desenvolvedora Front end
                        span(class="u-color-white u-db") Foregon - Trabalho
                        date(class="u-color-white") jan de 2024 - o momento

